<%= form_for(@company) do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
  <div class="field">
    <%= f.label :company_name %>
    <%= f.text_field :name %>

    <%= f.label :entitlements %>
    <%= f.text_field :entitlements %>
  </div>
  <div class="actions">
    <%= f.submit "Submit" %>
  </div>
<% end %>