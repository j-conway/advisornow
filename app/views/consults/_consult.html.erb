<tr>
  <td class="cases">
    <span class="content"><%= link_to consult.subject, [@company, consult], 'data-no-turbolink' => true %></span>
  </td>
  <td class="cases">
    <span class="content"><%= consult.updated_at %></span>
  </td>
  <% if consult.datascientist %>
    <td class="cases">
      <span class="content"><%= consult.datascientist.name %></span>
    </td>
  <% else %>
    <td class="cases">
      <span class="content">Unassigned</span>
    </td>
  <% end %>
  <td class="cases">
    <span class="content"><%= link_to consult.customer.name, consult.customer %></span>
  </td>
</tr>

