<% if @filter != "Open" %>
  <span class="ticket-toggle"><%= link_to "View Open Tickets" %></span>
<% else %>
  <span class="ticket-toggle"><%= link_to "View Closed Tickets", {:status => "Closed"} %></span>
<% end %>
<table class="table table-hover" summary="Consults">
  <thead>
    <tr>
      <th>Subject</th>
      <th>Last Updated</th>
      <th>Data Scientist</th>
      <th>Primary Customer</th>
    </tr>
  </thead>
  <tbody>

    
    <%= render @consults %>
    
  </tbody>
  
</table> 
